create    hillA,isegA,\
          nodeA[2],\
          inodeA[3]   

proc geometryA() { local i, j

					/* connect primary neurites */
    somaA connect hillA (0), 0.5
    hillA connect isegA (0), 1
    isegA connect inodeA[0] (0), 1
    inodeA[0] connect nodeA[0] (0), 1
    nodeA[0] connect inodeA[1] (0), 1
    inodeA[1] connect nodeA[1] (0), 1
    nodeA[1] connect inodeA[2] (0), 1

					/* neurite geometry*/
    hillA {  nseg=10
            pt3dclear()
            for j = 1, fscan() {
                pt3dadd(fscan(),fscan(),fscan(),fscan())
         }
    }

    isegA {  nseg=10
            pt3dclear()
            for j = 1, fscan() {
                pt3dadd(fscan(),fscan(),fscan(),fscan())
         }
    }

    inodeA[0] { nseg=25
            pt3dclear()
            for j = 1, fscan() {
                pt3dadd(fscan(),fscan(),fscan(),fscan())
         }
    }

    nodeA[0] { nseg=1
            pt3dclear()
            for j = 1, fscan() {
                pt3dadd(fscan(),fscan(),fscan(),fscan())
         }
    }

    inodeA[1] { nseg=25
            pt3dclear()
            for j = 1, fscan() {
                pt3dadd(fscan(),fscan(),fscan(),fscan())
         }
    }

    nodeA[1] { nseg=1
            pt3dclear()
            for j = 1, fscan() {
                pt3dadd(fscan(),fscan(),fscan(),fscan())
         }
    }

    inodeA[2] { nseg=25
            pt3dclear()
            for j = 1, fscan() {
                pt3dadd(fscan(),fscan(),fscan(),fscan())
         }
    }

}

geometryA()

